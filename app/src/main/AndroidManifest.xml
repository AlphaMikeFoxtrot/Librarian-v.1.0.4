<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anonymous.librarian">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IssueBookPhaseOne" />
        <activity android:name=".IssuedBookPhaseTwo" />
        <activity android:name=".IssueBookFinalPhase" />
        <activity android:name=".ReturnBook" />
        <activity android:name=".ReturnBookFinalPhase" />
        <activity android:name=".ViewSubscribers" />
        <activity android:name=".SubscriberDetails" />
        <activity android:name=".Analysis"></activity>
    </application>

</manifest>